{
    "name": "rajanvijayan/ai-crm-form",
    "description": "AI-powered CRM form generator for WordPress",
    "version": "1.2.0",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Rajan Vijayan",
            "email": "developer@example.com",
            "homepage": "https://github.com/rajanvijayan"
        }
    ],
    "homepage": "https://github.com/rajanvijayan/ai-crm-form",
    "support": {
        "issues": "https://github.com/rajanvijayan/ai-crm-form/issues",
        "source": "https://github.com/rajanvijayan/ai-crm-form"
    },
    "keywords": [
        "wordpress",
        "plugin",
        "forms",
        "crm",
        "ai",
        "lead-generation"
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "wp-coding-standards/wpcs": "^3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "squizlabs/php_codesniffer": "^3.8",
        "yoast/phpunit-polyfills": "^4.0"
    },
    "suggest": {
        "rajanvijayan/ai-engine": "Required for AI form generation features"
    },
    "autoload": {
        "psr-4": {
            "AICRMForm\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AICRMForm\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html tests/coverage/html",
        "phpcs": "phpcs --standard=phpcs.xml",
        "phpcbf": "phpcbf --standard=phpcs.xml",
        "lint": "@phpcs",
        "lint:fix": "@phpcbf",
        "audit": "composer audit",
        "install-tests": "bash bin/install-wp-tests.sh wordpress_test root '' localhost latest"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
